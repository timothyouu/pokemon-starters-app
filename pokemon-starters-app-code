//For homeScreen

//Music should automatically be playing
playSound("07-Professor-Oak_s-Laboratory.mp3", true);

//Clicking "symbolAudioUnmuted" should mute the music playing and change icon to "symbolAudioMuted"
onEvent("symbolAudioUnmuted", "click", function( ) {
  stopSound("07-Professor-Oak_s-Laboratory.mp3");
  hideElement("symbolAudioUnmuted");
  showElement("symbolAudioMuted");
});

//Clicking "symbolAudioMuted" should play the music and change icon to "symbolAudioUnmuted"
onEvent("symbolAudioMuted", "click", function( ) {
  playSound("07-Professor-Oak_s-Laboratory.mp3", true);
  hideElement("symbolAudioMuted");
  showElement("symbolAudioUnmuted");
});

// Clicking "imageSelectBulbasaur" takes user to screenBulbasaur and plays Bulbasaur sound
onEvent("imageSelectBulbasaur", "click", function( ) {
  setScreen("screenBulbasaur");
  playSound("001-bulbasaur.mp3", false);
});

// Clicking "imageSelectCharmander" takes user to screenCharmander and plays Charmander sound
onEvent("imageSelectCharmander", "click", function( ) {
  setScreen("screenCharmander");
  playSound("004-charmander.mp3", false);
});

